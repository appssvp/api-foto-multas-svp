{
    "openapi": "3.0.0",
    "info": {
        "title": "Fotomultas API",
        "description": "API para sistema de fotomultas",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000",
            "description": "Servidor de desarrollo"
        }
    ],
    "paths": {
        "/api/auth/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Autenticación",
                "operationId": "46776896f85b58fd1d3bf99927a93c44",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "admin@fotomultas.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "password123"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Login exitoso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "token": {
                                            "type": "string",
                                            "example": "1|abc123xyz..."
                                        },
                                        "user": {
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Credenciales inválidas"
                    }
                }
            }
        },
        "/api/detecciones": {
            "post": {
                "tags": [
                    "Detecciones"
                ],
                "summary": "Obtener detecciones de fotomultas",
                "operationId": "bab8430ca80f8325fd9e196e49b3be78",
                "requestBody": {
                    "required": false,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "limit": {
                                        "description": "Número de registros a retornar",
                                        "type": "integer",
                                        "example": 50
                                    },
                                    "offset": {
                                        "description": "Offset para paginación",
                                        "type": "integer",
                                        "example": 0
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Lista de detecciones",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "properties": {
                                            "recordId": {
                                                "type": "string"
                                            },
                                            "plateNum": {
                                                "type": "string"
                                            },
                                            "carSpeed": {
                                                "type": "integer"
                                            },
                                            "createTime": {
                                                "type": "string"
                                            },
                                            "channelInfoVO": {
                                                "type": "object"
                                            }
                                        },
                                        "type": "object"
                                    }
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/imagenes/{ticketId}": {
            "get": {
                "tags": [
                    "Imágenes"
                ],
                "summary": "Obtener imágenes de fotomulta",
                "operationId": "8878c16dec5252503065d51cf0c36f93",
                "parameters": [
                    {
                        "name": "ticketId",
                        "in": "path",
                        "description": "ID del ticket",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "img",
                        "in": "query",
                        "description": "Número de imagen (1, 2, o 3). Por defecto: 1",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "example": 1
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Imagen encontrada",
                        "content": {
                            "image/jpeg": {}
                        }
                    },
                    "404": {
                        "description": "Ticket o imagen no encontrada"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/auth/logout": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Cerrar sesión",
                "operationId": "01ad23fe04a1afe6b0ee09eb419c4345",
                "responses": {
                    "200": {
                        "description": "Logout exitoso"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    },
    "tags": [
        {
            "name": "Auth",
            "description": "Auth"
        },
        {
            "name": "Detecciones",
            "description": "Detecciones"
        },
        {
            "name": "Imágenes",
            "description": "Imágenes"
        }
    ]
}