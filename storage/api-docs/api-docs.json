{
    "openapi": "3.0.0",
    "info": {
        "title": "Fotomultas API",
        "description": "API para sistema de fotomultas",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000",
            "description": "Servidor de desarrollo"
        }
    ],
    "paths": {
        "/api/auth/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Autenticación",
                "operationId": "46776896f85b58fd1d3bf99927a93c44",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "admin@fotomultas.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "password123"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Login exitoso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "token": {
                                            "type": "string",
                                            "example": "1|abc123xyz..."
                                        },
                                        "user": {
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Credenciales inválidas"
                    }
                }
            }
        },
        "/api/detecciones": {
            "post": {
                "tags": [
                    "Detecciones"
                ],
                "summary": "Obtener detecciones de fotomultas",
                "operationId": "bab8430ca80f8325fd9e196e49b3be78",
                "requestBody": {
                    "required": false,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "limit": {
                                        "description": "Número de registros a retornar",
                                        "type": "integer",
                                        "example": 50
                                    },
                                    "offset": {
                                        "description": "Offset para paginación",
                                        "type": "integer",
                                        "example": 0
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Lista de detecciones",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "properties": {
                                            "recordId": {
                                                "type": "string"
                                            },
                                            "plateNum": {
                                                "type": "string"
                                            },
                                            "carSpeed": {
                                                "type": "integer"
                                            },
                                            "createTime": {
                                                "type": "string"
                                            },
                                            "channelInfoVO": {
                                                "type": "object"
                                            }
                                        },
                                        "type": "object"
                                    }
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/imagenes": {
            "post": {
                "tags": [
                    "Imágenes"
                ],
                "summary": "Endpoint de imágenes",
                "operationId": "e95f28f47334dd016a5f2e9534e5744a",
                "responses": {
                    "200": {
                        "description": "Imágenes procesadas"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/auth/logout": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Cerrar sesión",
                "operationId": "01ad23fe04a1afe6b0ee09eb419c4345",
                "responses": {
                    "200": {
                        "description": "Logout exitoso"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    },
    "tags": [
        {
            "name": "Auth",
            "description": "Auth"
        },
        {
            "name": "Detecciones",
            "description": "Detecciones"
        },
        {
            "name": "Imágenes",
            "description": "Imágenes"
        }
    ]
}